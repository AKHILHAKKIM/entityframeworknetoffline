            <span class="kwrd">return</span> ((<span class="userclass">IObjectContextAdapter</span>)<span class="kwrd">this</span>).ObjectContext.ExecuteFunction&lt;Nullable&lt;<span class="kwrd">decimal</span>&gt;&gt;(<span class="str">"sp_InsertStudentInfo"</span>, standardIdParameter, studentNameParameter);
        }
    
