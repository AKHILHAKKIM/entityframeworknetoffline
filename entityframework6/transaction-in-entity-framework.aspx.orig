    <span class="kwrd">public</span> SchoolContext(): base("SchoolDB")
    {
        Database.SetInitializer&lt;SchoolContext&gt;(<span class="kwrd">new</span> CreateDatabaseIfNotExists&lt;SchoolContext&gt;());
    }

    public DbSet&lt;Student&gt; Students { get; set; }
    public DbSet&lt;Standard&gt; Grades { get; set; }
    public DbSet&lt;Course&gt; Courses { get; set; }
}
