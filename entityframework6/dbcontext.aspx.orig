            <span class="kwrd">return</span> ((<span class="userclass">IObjectContextAdapter</span>)<span class="kwrd">this</span>).ObjectContext.ExecuteFunction&lt;GetCoursesByStudentId_Result&gt;(<span class="str">"GetCoursesByStudentId"</span>, studentIdParameter);
        }
    
        <span class="kwrd">public</span> <span class="kwrd">virtual</span> <span class="kwrd">int</span> sp_DeleteStudent(<span class="kwrd">Nullable</span>&lt;<span class="kwrd">int</span>&gt; studentId)
        {
            <span class="kwrd">var</span> studentIdParameter = studentId.HasValue ?
                <span class="kwrd">new</span> <span class="userclass">ObjectParameter</span>(<span class="str">"StudentId"</span>, studentId) :
                <span class="kwrd">new</span> <span class="userclass">ObjectParameter</span>(<span class="str">"StudentId"</span>, <span class="kwrd">typeof</span>(<span class="kwrd">int</span>));
    
            <span class="kwrd">return</span> ((<span class="userclass">IObjectContextAdapter</span>)<span class="kwrd">this</span>).ObjectContext.ExecuteFunction(<span class="str">"sp_DeleteStudent"</span>, studentIdParameter);
        }
    
        <span class="kwrd">public</span> <span class="kwrd">virtual</span> <span class="userclass">ObjectResult</span>&lt;<span class="kwrd">Nullable</span>&lt;<span class="kwrd">decimal</span>&gt;&gt; sp_InsertStudentInfo(Nullable&lt;<span class="kwrd">int</span>&gt; standardId, <span class="kwrd">string</span> studentName)
        {
