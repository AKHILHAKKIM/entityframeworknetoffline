        {
            <span class="kwrd">throw</span> <span class="kwrd">new</span> <span class="userclass">UnintentionalCodeFirstException</span>();
        }
    
        <span class="kwrd">public</span> <span class="kwrd">virtual</span> <span class="userclass">DbSet</span>&lt;<span class="userclass">Course</span>&gt; Courses { <span class="kwrd">get</span>; <span class="kwrd">set</span>; }
        <span class="kwrd">public</span> <span class="kwrd">virtual</span> <span class="userclass">DbSet</span>&lt;<span class="userclass">Standard</span>&gt; Standards { <span class="kwrd">get</span>; <span class="kwrd">set</span>; }
        <span class="kwrd">public</span> <span class="kwrd">virtual</span> <span class="userclass">DbSet</span>&lt;<span class="userclass">Student</span>&gt; Students { <span class="kwrd">get</span>; <span class="kwrd">set</span>; }
        <span class="kwrd">public</span> <span class="kwrd">virtual</span> <span class="userclass">DbSet</span>&lt;<span class="userclass">StudentAddress</span>&gt; StudentAddresses { <span class="kwrd">get</span>; <span class="kwrd">set</span>; }
        <span class="kwrd">public</span> <span class="kwrd">virtual</span> <span class="userclass">DbSet</span>&lt;<span class="userclass">Teacher</span>&gt; Teachers { <span class="kwrd">get</span>; <span class="kwrd">set</span>; }
        <span class="kwrd">public</span> <span class="kwrd">virtual</span> <span class="userclass">DbSet</span>&lt;<span class="userclass">View_StudentCourse</span>&gt; View_StudentCourse { <span class="kwrd">get</span>; <span class="kwrd">set</span>; }
    
        <span class="kwrd">public</span> <span class="kwrd">virtual</span> <span class="userclass">ObjectResult</span>&lt;<span class="userclass">GetCoursesByStudentId_Result</span>&gt; GetCoursesByStudentId(<span class="kwrd">Nullable</span>&lt;<span class="kwrd">int</span>&gt; studentId)
        {
            <span class="kwrd">var</span> studentIdParameter = studentId.HasValue ?
                <span class="kwrd">new</span> <span class="userclass">ObjectParameter</span>(<span class="str">"StudentId"</span>, studentId) :
